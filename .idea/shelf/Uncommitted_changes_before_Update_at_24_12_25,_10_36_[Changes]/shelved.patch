+  "script": "\n// ============================================================================\n// STOCK ENTRY - CUSTOM LOGIC (PROFESSIONAL VERSION)\n// ============================================================================\n// Ssenariylar:\n//   A) \"Расход по заказу\" - Ishlab chiqarish chiqimi (BOM asosida)\n//   B) \"Услуги по заказу\" - Tashqi xizmatlar\n//   C) \"Перемещение\" - Sub-kompaniyalar o'rtasida transfer\n// ============================================================================\n\nfrappe.ui.form.on('Stock Entry', {\n\tsetup: function(frm) {\n\t\t// SETUP: Formni birinchi marta ochilganda filtrlarni o'rnatish\n\n\t\t// 1. Stock Entry Type filtri (Faqat 3 ta kerakli tur)\n\t\tfrm.set_query('stock_entry_type', function() {\n\t\t\treturn {\n\t\t\t\tfilters: {\n\t\t\t\t\t'name': ['in', ['Услуги по заказу', 'Расход по заказу', 'Перемещение']]\n\t\t\t\t}\n\t\t\t};\n\t\t});\n\n\t\t// 2. Sales Order Item filtri (Sales Order ga bog'liq)\n\t\tfrm.set_query('custom_sales_order_item', function() {\n\t\t\tif (!frm.doc.custom_sales_order) {\n\t\t\t\tfrappe.msgprint(__('Avval Sales Order tanlang'));\n\t\t\t\treturn { filters: { 'name': ['=', ''] } }; // Bo'sh\n\t\t\t}\n\t\t\treturn {\n\t\t\t\tquery: 'premierprint.utils.stock_entry.get_sales_order_items_query',\n\t\t\t\tfilters: {\n\t\t\t\t\tsales_order: frm.doc.custom_sales_order\n\t\t\t\t}\n\t\t\t};\n\t\t});\n\t},\n\n\tonload: function(frm) {\n\t\t// ONLOAD: Hujjat yuklanganda UI qoidalarini qo'llash\n\t\tapply_ui_rules(frm);\n\t},\n\n\trefresh: function(frm) {\n\t\t// REFRESH: Har safar yangilanganda UI qoidalarini qo'llash\n\t\tapply_ui_rules(frm);\n\t},\n\n\tstock_entry_type: function(frm) {\n\t\t// TYPE O'ZGARGANDA:\n\t\t// 1. UI qoidalarini qayta qo'llash\n\t\tapply_ui_rules(frm);\n\n\t\t// 2. Agar \"Перемещение\" bo'lmasa, Sales Order maydonlarini tozalash\n\t\tif (frm.doc.stock_entry_type !== 'Расход по заказу' && frm.doc.stock_entry_type !== 'Услуги по заказу') {\n\t\t\tfrm.set_value('custom_sales_order', '');\n\t\t\tfrm.set_value('custom_sales_order_item', '');\n\t\t}\n\n\t\t// 3. Agar \"Перемещение\" bo'lmasa, Sub-Company maydonlarini tozalash\n\t\tif (frm.doc.stock_entry_type !== 'Перемещение') {\n\t\t\tfrm.set_value('custom_from_sub_company', '');\n\t\t\tfrm.set_value('custom_to_sub_company', '');\n\t\t}\n\t},\n\n\tcustom_sales_order: function(frm) {\n\t\t// SALES ORDER O'ZGARGANDA:\n\t\t// 1. Sales Order Item ni tozalash\n\t\tfrm.set_value('custom_sales_order_item', '');\n\n\t\t// 2. Items jadvalini tozalash\n\t\tfrm.clear_table('items');\n\t\tfrm.refresh_field('items');\n\t},\n\n\tcustom_sales_order_item: function(frm) {\n\t\t// SALES ORDER ITEM TANLANGANDA: BOM materiallarini yuklash\n\n\t\tif (!frm.doc.custom_sales_order_item) {\n\t\t\treturn;\n\t\t}\n\n\t\t// Loading indikatorini ko'rsatish\n\t\tfrappe.dom.freeze(__('BOM materiallari yuklanmoqda...'));\n\n\t\tfrappe.call({\n\t\t\tmethod: 'premierprint.utils.stock_entry.get_bom_materials',\n\t\t\targs: {\n\t\t\t\tsales_order_item_id: frm.doc.custom_sales_order_item\n\t\t\t},\n\t\t\tcallback: function(r) {\n\t\t\t\tfrappe.dom.unfreeze();\n\n\t\t\t\tif (r.message && r.message.length > 0) {\n\t\t\t\t\t// Avvalgi itemlarni tozalash\n\t\t\t\t\tfrm.clear_table('items');\n\n\t\t\t\t\t// Yangi materiallarni qo'shish\n\t\t\t\t\tr.message.forEach(function(item) {\n\t\t\t\t\t\tlet row = frm.add_child('items');\n\t\t\t\t\t\trow.item_code = item.item_code;\n\t\t\t\t\t\trow.item_name = item.item_name;\n\t\t\t\t\t\trow.description = item.description;\n\t\t\t\t\t\trow.qty = item.qty;\n\t\t\t\t\t\trow.uom = item.uom;\n\t\t\t\t\t\trow.stock_uom = item.stock_uom;\n\t\t\t\t\t\trow.conversion_factor = item.conversion_factor;\n\t\t\t\t\t\trow.s_warehouse = item.s_warehouse;\n\t\t\t\t\t\trow.cost_center = item.cost_center;\n\t\t\t\t\t\t// basic_rate va amount qo'shmaslik - ular yashirin\n\t\t\t\t\t});\n\n\t\t\t\t\tfrm.refresh_field('items');\n\n\t\t\t\t\tfrappe.show_alert({\n\t\t\t\t\t\tmessage: __(`${r.message.length} 